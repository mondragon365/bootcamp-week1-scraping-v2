{
  "name": "linkedin comunication",
  "description": "Extract Profile Yeh!",
  "version": "1.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["./app.js"]
    }
  ],
  "action":{
    "default_popup":"./App/index.html",
    "default_icon": {
      "32": "./App/Image/32.png",
      "48": "./App/Image/48.png"
    }
  },
  "permissions": ["activeTab", "scripting","storage"],
  "icons": {
    "32": "./App/Image/32.png",
    "48": "./App/Image/48.png"
  }
}